<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.ccode.sdaschoolbahraich">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".FeesActivity"
            android:theme="@style/FeesActivityTheme"></activity>
        <activity
            android:name=".CircularsActivity"
            android:theme="@style/CircularsActivityTheme"/>
        <activity
            android:name=".MarksActivity"
            android:theme="@style/MarksActivityTheme"/>
        <activity
            android:name=".CalendarActivity"
            android:theme="@style/CalendarActivityTheme"/>
        <activity
            android:name=".AssignmentsActivity"
            android:theme="@style/AssignmentActivityTheme" />
        <activity
            android:name=".AttendanceActivity"
            android:theme="@style/AttendanceActivityTheme" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/MainActivityTheme" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>